{
  "check-coverage": true,
  "per-file": true,
  "lines": 99,
  "statements": 99,
  "functions": 99,
  "branches": 99,
  "include": [
    "*.js"
  ],
  "exclude": [
    "spec/**/*.js",
    "*.config.js"
  ],
  "reporter": [
    "lcov",
    "text",
    "html"
  ],
  "cache": true,
  "all": true,
  "temp-dir": "./coverage/tmp",
  "report-dir": "./coverage"
}
